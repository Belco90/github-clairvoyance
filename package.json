{
  "name": "octoclairvoyant",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "dev": "next",
    "build": "next build",
    "serve": "next start",
    "smoketest": "yarn type-check && yarn lint && yarn format:check && yarn build",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true yarn build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "prettier-base": "prettier . --ignore-path .gitignore --ignore-unknown",
    "format": "yarn prettier-base --write",
    "format:check": "yarn prettier-base --check",
    "gen:theme-typings": "chakra-cli tokens src/customTheme.ts",
    "prepare": "husky install",
    "postinstall": "yarn gen:theme-typings"
  },
  "dependencies": {
    "@chakra-ui/react": "1.6.5",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@next/bundle-analyzer": "11.0.1",
    "@octokit/auth-callback": "2.0.1",
    "@octokit/rest": "18.6.7",
    "@sentry/nextjs": "6.9.0",
    "downshift": "6.1.3",
    "focus-visible": "5.2.0",
    "framer-motion": "4.1.17",
    "lodash": "4.17.21",
    "next": "11.0.1",
    "next-seo": "4.26.0",
    "nookies": "2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0",
    "react-query": "3.18.1",
    "rehype-highlight": "4.1.0",
    "rehype-react": "6.2.1",
    "remark-gfm": "1.0.0",
    "remark-github": "10.1.0",
    "remark-parse": "9.0.0",
    "remark-rehype": "8.1.0",
    "remark-stringify": "9.0.1",
    "semver": "7.3.5",
    "title": "3.4.3",
    "unified": "9.2.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "1.5.0",
    "@types/gtag.js": "0.0.7",
    "@types/lodash": "4.14.171",
    "@types/node": "14.17.5",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/semver": "7.3.7",
    "@types/unist": "2.0.5",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.30.0",
    "eslint-config-kentcdodds": "19.0.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "type-fest": "1.2.2",
    "typescript": "4.3.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
