{
	"name": "octoclairvoyant-webapp",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"pnpm": "^8.4.0"
	},
	"packageManager": "pnpm@8.4.0",
	"scripts": {
		"start": "pnpm dev",
		"dev": "next dev",
		"build": "next build",
		"serve": "next start",
		"type-check": "tsc --project tsconfig.validation.json",
		"type-check:cypress": "tsc --project cypress/tsconfig.json",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "pnpm lint --fix",
		"prettier-base": "prettier . --ignore-unknown --cache --log-level warn",
		"format": "pnpm prettier-base --write",
		"format:check": "pnpm prettier-base --check",
		"test": "jest",
		"test:watch": "pnpm test -- --watch",
		"test:ci": "pnpm test -- --ci",
		"cy:open": "cypress open",
		"cy:headless": "cypress run",
		"e2e": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:open --e2e'",
		"e2e:dev": "start-server-and-test 'pnpm dev' http://localhost:3000 'pnpm cy:open --e2e'",
		"e2e:headless": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:headless'",
		"gen:theme-typings": "chakra-cli tokens src/customTheme.ts --strict-component-types --strict-token-types",
		"prepare": "is-ci || husky install",
		"postinstall": "pnpm gen:theme-typings"
	},
	"dependencies": {
		"@chakra-ui/next-js": "2.1.5",
		"@chakra-ui/react": "2.8.1",
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"@octokit/auth-callback": "4.0.0",
		"@octokit/rest": "20.0.2",
		"@sentry/nextjs": "7.73.0",
		"@tanstack/react-query": "4.35.7",
		"@tanstack/react-query-devtools": "4.35.7",
		"@vercel/analytics": "1.1.0",
		"downshift": "8.2.2",
		"framer-motion": "10.16.4",
		"lodash-es": "4.17.21",
		"next": "13.5.4",
		"next-seo": "6.1.0",
		"nookies": "2.5.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "4.11.0",
		"rehype-highlight": "6.0.0",
		"rehype-react": "7.2.0",
		"remark-emoji": "4.0.0",
		"remark-gfm": "3.0.1",
		"remark-github": "12.0.0",
		"remark-parse": "10.0.2",
		"remark-rehype": "11.0.0",
		"remark-stringify": "10.0.3",
		"semver": "7.5.4",
		"unified": "10.1.2"
	},
	"devDependencies": {
		"@chakra-ui/cli": "2.4.1",
		"@chakra-ui/styled-system": "2.9.1",
		"@chakra-ui/theme-tools": "2.1.1",
		"@chakra-ui/utils": "2.0.14",
		"@jest/globals": "29.7.0",
		"@octokit/openapi-types": "19.0.0",
		"@testing-library/cypress": "10.0.1",
		"@testing-library/react": "14.0.0",
		"@types/happo-cypress": "4.1.1",
		"@types/lodash-es": "4.17.9",
		"@types/mdast": "3.0.13",
		"@types/node": "18.18.4",
		"@types/react": "18.2.27",
		"@types/react-dom": "18.2.12",
		"@types/semver": "7.5.3",
		"@types/unist": "3.0.0",
		"@typescript-eslint/eslint-plugin": "6.7.5",
		"@typescript-eslint/parser": "6.7.5",
		"cypress": "13.3.0",
		"eslint": "8.51.0",
		"eslint-config-next": "13.5.4",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-cypress": "2.15.1",
		"eslint-plugin-jest": "27.4.2",
		"eslint-plugin-jest-formatting": "3.1.0",
		"eslint-plugin-unicorn": "48.0.1",
		"happo-cypress": "4.1.1",
		"happo-e2e": "2.2.5",
		"happo.io": "8.4.4",
		"husky": "8.0.3",
		"is-ci": "3.0.1",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"lint-staged": "14.0.1",
		"msw": "1.2.5",
		"prettier": "3.0.3",
		"start-server-and-test": "2.0.1",
		"ts-jest": "29.1.1",
		"ts-node": "10.9.1",
		"type-fest": "4.3.3",
		"typescript": "5.2.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"msw": {
		"workerDirectory": "public"
	}
}
