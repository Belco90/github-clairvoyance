{
	"name": "octoclairvoyant-webapp",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"pnpm": "^7.13.0"
	},
	"packageManager": "pnpm@7.14.0",
	"scripts": {
		"start": "pnpm dev",
		"dev": "next dev",
		"build": "next build",
		"serve": "next start",
		"type-check": "tsc --project tsconfig.validation.json",
		"type-check:cypress": "tsc --project cypress/tsconfig.json",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "pnpm lint --fix",
		"prettier-base": "prettier . --ignore-unknown --cache --loglevel warn",
		"format": "pnpm prettier-base --write",
		"format:check": "pnpm prettier-base --check",
		"test": "jest",
		"test:watch": "pnpm test -- --watch",
		"test:ci": "pnpm test -- --ci",
		"cy:open": "cypress open",
		"cy:headless": "cypress run",
		"e2e": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:open --e2e'",
		"e2e:dev": "start-server-and-test 'pnpm dev' http://localhost:3000 'pnpm cy:open --e2e'",
		"e2e:headless": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:headless'",
		"gen:theme-typings": "chakra-cli tokens src/customTheme.ts --strict-component-types",
		"prepare": "is-ci || husky install",
		"postinstall": "pnpm gen:theme-typings"
	},
	"dependencies": {
		"@chakra-ui/react": "2.3.6",
		"@emotion/react": "11.10.5",
		"@emotion/styled": "11.10.5",
		"@octokit/auth-callback": "3.0.0",
		"@octokit/rest": "19.0.5",
		"@sentry/nextjs": "7.17.2",
		"@tanstack/react-query": "4.13.4",
		"@tanstack/react-query-devtools": "4.13.4",
		"@vercel/analytics": "0.1.3",
		"downshift": "7.0.1",
		"framer-motion": "7.6.2",
		"lodash-es": "4.17.21",
		"next": "12.3.2-canary.39",
		"next-seo": "5.11.1",
		"nookies": "2.5.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "4.6.0",
		"rehype-highlight": "6.0.0",
		"rehype-react": "7.1.1",
		"remark-gfm": "3.0.1",
		"remark-github": "11.2.4",
		"remark-parse": "10.0.1",
		"remark-rehype": "10.1.0",
		"remark-stringify": "10.0.2",
		"semver": "7.3.8",
		"unified": "10.1.2"
	},
	"devDependencies": {
		"@chakra-ui/cli": "2.1.8",
		"@chakra-ui/styled-system": "2.3.4",
		"@chakra-ui/theme-tools": "2.0.12",
		"@chakra-ui/utils": "2.0.11",
		"@jest/globals": "29.2.2",
		"@octokit/openapi-types": "14.0.0",
		"@testing-library/cypress": "8.0.3",
		"@testing-library/react": "13.4.0",
		"@types/happo-cypress": "3.0.1",
		"@types/lodash-es": "4.17.6",
		"@types/mdast": "3.0.10",
		"@types/node": "16.11.56",
		"@types/react": "18.0.24",
		"@types/react-dom": "18.0.8",
		"@types/semver": "7.3.13",
		"@types/unist": "2.0.6",
		"@typescript-eslint/eslint-plugin": "5.41.0",
		"@typescript-eslint/parser": "5.41.0",
		"cypress": "10.11.0",
		"eslint": "8.26.0",
		"eslint-config-next": "12.3.2-canary.39",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-cypress": "2.12.1",
		"eslint-plugin-jest": "27.1.3",
		"eslint-plugin-jest-formatting": "3.1.0",
		"eslint-plugin-unicorn": "44.0.2",
		"happo-cypress": "4.0.1",
		"happo-e2e": "2.0.0",
		"happo.io": "8.2.0",
		"husky": "8.0.1",
		"is-ci": "3.0.1",
		"jest": "29.2.2",
		"jest-environment-jsdom": "29.2.2",
		"lint-staged": "13.0.3",
		"msw": "0.47.4",
		"prettier": "2.7.1",
		"start-server-and-test": "1.14.0",
		"ts-jest": "29.0.3",
		"type-fest": "3.1.0",
		"typescript": "4.8.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"msw": {
		"workerDirectory": "public"
	}
}
