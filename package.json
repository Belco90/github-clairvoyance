{
	"name": "octoclairvoyant-webapp",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"pnpm": "^8.4.0"
	},
	"packageManager": "pnpm@8.4.0",
	"scripts": {
		"start": "pnpm dev",
		"dev": "next dev",
		"build": "next build",
		"serve": "next start",
		"type-check": "tsc --project tsconfig.validation.json",
		"type-check:cypress": "tsc --project cypress/tsconfig.json",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "pnpm lint --fix",
		"prettier-base": "prettier . --ignore-unknown --cache --loglevel warn",
		"format": "pnpm prettier-base --write",
		"format:check": "pnpm prettier-base --check",
		"test": "jest",
		"test:watch": "pnpm test -- --watch",
		"test:ci": "pnpm test -- --ci",
		"cy:open": "cypress open",
		"cy:headless": "cypress run",
		"e2e": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:open --e2e'",
		"e2e:dev": "start-server-and-test 'pnpm dev' http://localhost:3000 'pnpm cy:open --e2e'",
		"e2e:headless": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:headless'",
		"gen:theme-typings": "chakra-cli tokens src/customTheme.ts --strict-component-types --strict-token-types",
		"prepare": "is-ci || husky install",
		"postinstall": "pnpm gen:theme-typings"
	},
	"dependencies": {
		"@chakra-ui/react": "2.6.0",
		"@emotion/react": "11.11.0",
		"@emotion/styled": "11.11.0",
		"@octokit/auth-callback": "3.0.0",
		"@octokit/rest": "19.0.7",
		"@sentry/nextjs": "7.51.0",
		"@tanstack/react-query": "4.29.5",
		"@tanstack/react-query-devtools": "4.29.6",
		"@vercel/analytics": "1.0.1",
		"downshift": "7.6.0",
		"framer-motion": "10.12.8",
		"lodash-es": "4.17.21",
		"next": "13.2.4",
		"next-seo": "6.0.0",
		"nookies": "2.5.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "4.8.0",
		"rehype-highlight": "6.0.0",
		"rehype-react": "7.2.0",
		"remark-gfm": "3.0.1",
		"remark-github": "11.2.4",
		"remark-parse": "10.0.1",
		"remark-rehype": "10.1.0",
		"remark-stringify": "10.0.2",
		"semver": "7.5.0",
		"unified": "10.1.2"
	},
	"devDependencies": {
		"@chakra-ui/cli": "2.4.0",
		"@chakra-ui/styled-system": "2.9.0",
		"@chakra-ui/theme-tools": "2.0.16",
		"@chakra-ui/utils": "2.0.14",
		"@jest/globals": "29.5.0",
		"@octokit/openapi-types": "17.1.0",
		"@testing-library/cypress": "9.0.0",
		"@testing-library/react": "14.0.0",
		"@types/happo-cypress": "4.1.0",
		"@types/lodash-es": "4.17.7",
		"@types/mdast": "3.0.11",
		"@types/node": "18.16.3",
		"@types/react": "18.2.5",
		"@types/react-dom": "18.2.3",
		"@types/semver": "7.3.13",
		"@types/unist": "2.0.6",
		"@typescript-eslint/eslint-plugin": "5.57.1",
		"@typescript-eslint/parser": "5.57.1",
		"cypress": "12.11.0",
		"eslint": "8.37.0",
		"eslint-config-next": "13.2.4",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-cypress": "2.13.2",
		"eslint-plugin-jest": "27.2.1",
		"eslint-plugin-jest-formatting": "3.1.0",
		"eslint-plugin-unicorn": "46.0.0",
		"happo-cypress": "4.1.1",
		"happo-e2e": "2.2.4",
		"happo.io": "8.3.2",
		"husky": "8.0.3",
		"is-ci": "3.0.1",
		"jest": "29.5.0",
		"jest-environment-jsdom": "29.5.0",
		"lint-staged": "13.2.2",
		"msw": "1.2.1",
		"prettier": "2.8.7",
		"start-server-and-test": "2.0.0",
		"ts-jest": "29.1.0",
		"type-fest": "3.10.0",
		"typescript": "5.0.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"msw": {
		"workerDirectory": "public"
	}
}
