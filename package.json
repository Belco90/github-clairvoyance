{
	"name": "octoclairvoyant-webapp",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"pnpm": "^7.5.0"
	},
	"packageManager": "pnpm@7.5.0",
	"scripts": {
		"start": "pnpm dev",
		"dev": "next",
		"build": "next build",
		"serve": "next start",
		"smoketest": "pnpm type-check && pnpm lint && pnpm format:check && pnpm build",
		"type-check": "tsc --project tsconfig.validation.json",
		"type-check:cypress": "tsc --project cypress/tsconfig.json",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "pnpm lint --fix",
		"prettier-base": "prettier . --ignore-unknown --cache --loglevel warn",
		"format": "pnpm prettier-base --write",
		"format:check": "pnpm prettier-base --check",
		"test": "jest",
		"test:watch": "pnpm test -- --watch",
		"test:ci": "pnpm test -- --ci",
		"cy:open": "cypress open",
		"cy:headless": "cypress run",
		"e2e": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:open'",
		"e2e:dev": "start-server-and-test 'pnpm dev' http://localhost:3000 'pnpm cy:open'",
		"e2e:headless": "start-server-and-test 'pnpm build && pnpm serve' http://localhost:3000 'pnpm cy:headless'",
		"gen:theme-typings": "chakra-cli tokens src/customTheme.ts --strict-component-types",
		"prepare": "is-ci || husky install",
		"postinstall": "pnpm gen:theme-typings"
	},
	"dependencies": {
		"@chakra-ui/react": "2.2.9",
		"@emotion/react": "11.10.0",
		"@emotion/styled": "11.10.0",
		"@octokit/auth-callback": "3.0.0",
		"@octokit/rest": "19.0.4",
		"@sentry/nextjs": "7.11.1",
		"@tanstack/react-query": "4.2.3",
		"@tanstack/react-query-devtools": "4.2.3",
		"downshift": "6.1.9",
		"framer-motion": "7.2.1",
		"lodash-es": "4.17.21",
		"next": "12.2.5",
		"next-seo": "5.5.0",
		"nookies": "2.5.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "4.4.0",
		"rehype-highlight": "5.0.2",
		"rehype-react": "7.1.1",
		"remark-gfm": "3.0.1",
		"remark-github": "11.2.4",
		"remark-parse": "10.0.1",
		"remark-rehype": "10.1.0",
		"remark-stringify": "10.0.2",
		"semver": "7.3.7",
		"unified": "10.1.2"
	},
	"devDependencies": {
		"@chakra-ui/cli": "2.1.6",
		"@chakra-ui/styled-system": "2.2.7",
		"@chakra-ui/theme-tools": "2.0.9",
		"@chakra-ui/utils": "2.0.8",
		"@jest/globals": "28.1.3",
		"@octokit/openapi-types": "13.0.1",
		"@testing-library/cypress": "8.0.3",
		"@types/happo-cypress": "3.0.1",
		"@types/lodash": "4.14.182",
		"@types/mdast": "3.0.10",
		"@types/node": "16.11.47",
		"@types/react": "18.0.17",
		"@types/react-dom": "18.0.6",
		"@types/semver": "7.3.12",
		"@types/unist": "2.0.6",
		"@typescript-eslint/eslint-plugin": "5.33.1",
		"@typescript-eslint/parser": "5.33.1",
		"cypress": "10.6.0",
		"eslint": "8.22.0",
		"eslint-config-next": "12.2.5",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-cypress": "2.12.1",
		"eslint-plugin-jest": "27.0.1",
		"eslint-plugin-jest-formatting": "3.1.0",
		"eslint-plugin-unicorn": "43.0.2",
		"happo-cypress": "3.0.1",
		"happo-e2e": "1.2.0",
		"happo.io": "7.3.0",
		"husky": "8.0.1",
		"is-ci": "3.0.1",
		"jest": "28.1.3",
		"lint-staged": "13.0.3",
		"prettier": "2.7.1",
		"start-server-and-test": "1.14.0",
		"type-fest": "2.18.0",
		"typescript": "4.7.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
